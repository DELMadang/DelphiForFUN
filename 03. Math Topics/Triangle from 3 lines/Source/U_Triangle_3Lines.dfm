  object Memo2: TMemo
    Left = 8
    Top = 8
    Width = 497
    Height = 385
    Color = 14548991
    Font.Charset = DEFAULT_CHARSET
    Font.Color = clWindowText
    Font.Height = -16
    Font.Name = 'MS Sans Serif'
    Font.Style = []
    Lines.Strings = (
      
        'Given three line segments, construct a triangle if possible by d' +
        'efining '
      'the coordinates of the three corners.'
      ''
      
        'Here'#39's the math to determine the coordinates of the third point ' +
        'if the '
      
        'line labeled L1 (and with length L1) is placed horizontally star' +
        'ting at '
      'coordinates (0,0).'
      ''
      
        '1. L2^2 = s^2 + a^2                                          (Py' +
        'thagoras)'
      
        '2. L3^2 = (L1-s)^2 + a^2                                   (Pyth' +
        'agoras)'
      
        '3. L2^2 - s^2  = L3^2 -(L1- s)^2          (Identity - solve 1 & ' +
        '2 for a^2)'
      '4  L2^2 - s^2 = L3^2 - L1^2 + 2*L1*s - s^2   (Expand 3.)'
      
        '5. s = { L2^2 + L1^2 - L3^2) } / (2*L1)          (solve 4. for "' +
        's")'
      
        '6. a = sqrt{ L2^2 - s^2 }                                     (s' +
        'olve 1. for "a")'
      ''
      
        'You can view the effect of changing side lengths by clicking the' +
        ' '
      'L1, L2, L3 arrows  in the lower right portion of this page.  '
      ' ')
    ParentFont = False
    TabOrder = 1
  end
