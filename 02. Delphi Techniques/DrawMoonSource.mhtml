<b>unit</b> U_DrawMoon;

<b>interface</b>

<b>uses</b>
Windows, Messages, SysUtils, Classes, Graphics, Controls, Forms, Dialogs,
  ExtCtrls, StdCtrls;

<b>type</b>
TForm1 =3D <b>class</b>(TForm)
    PBox: TPaintBox;
    CloseBtn: TButton;
    MoonBtn: TButton;
<b>procedure</b> FormActivate(Sender: TObject);
<b>procedure</b> PBoxPaint(Sender: TObject);
<b>procedure</b> CloseBtnClick(Sender: TObject);
<b>procedure</b> FormClose(Sender: TObject; <b>var</b> Action: TCloseAction=
);
<b>procedure</b> MoonBtnClick(Sender: TObject);
<b>private</b>
<font color=3D"#003399"><i>{ Private declarations }</i></font>
<b>public</b>
moonphase:single; <font color=3D"#003399"><i>{fraction of moon visible:  0 =
to 1}</i></font>
runflag:boolean; <font color=3D"#003399"><i>{controls program stopping}</i>=
</font>
waxing:boolean;  <font color=3D"#003399"><i>{true =3D=3D&gt; moon visibilit=
y is increasing}</i></font>
b:TBitmap;       <font color=3D"#003399"><i>{moon image}</i></font>
showmoon:boolean; <font color=3D"#003399"><i>{flag}</i></font>

<font color=3D"#003399"><i>{moon image coordinates}</i></font>
lx,rx,ty,by:integer; <font color=3D"#003399"><i>{corners of moon image}</i>=
</font>
cx,cy:integer; <font color=3D"#003399"><i>{center of moon image}</i></font>
rad:integer; <font color=3D"#003399"><i>{radius}</i></font>
<b>end</b>;

<b>var</b>
Form1: TForm1;

<b>implementation</b>
<b>uses</b> math;

<font color=3D"#003399"><i>{$R *.DFM}</i></font>

<font color=3D"#003399"><i>{******************* FormActivate **************=
**}</i></font>
<b>procedure</b> TForm1.FormActivate(Sender: TObject);
<b>var</b>
inc:single; <font color=3D"#003399"><i>{angle - radian increment for each v=
iew}</i></font>
angle:single;  <font color=3D"#003399"><i>{the angle of sun}</i></font>
fname:<b>string</b>;
  i,j:integer;
<b>begin</b>
<b>with</b> pbox <b>do</b>
<b>begin</b> <font color=3D"#003399"><i>{set moon image dimensions}</i></fo=
nt>
lx:=3D3;  <font color=3D"#003399"><i>{left x}</i></font>
ty:=3D2;  <font color=3D"#003399"><i>{top y}</i></font>
rx:=3Dwidth-lx; <font color=3D"#003399"><i>{right x}</i></font>
by:=3Dheight-ty;<font color=3D"#003399"><i>{bottom y}</i></font>
cx:=3Dwidth <b>div</b> 2; <font color=3D"#003399"><i>{center}</i></font>
cy:=3Dheight <b>div</b> 2;
    rad:=3D cx-lx;<font color=3D"#003399"><i>{radius}</i></font>
<b>end</b>;
  showmoon:=3Dfalse; <font color=3D"#003399"><i>{no moon image initially}</=
i></font>
fname:=3Dextractfilepath(application.exename)+<font color=3D"#9933CC">'Brig=
htFullMoon.bmp'</font>;
<b>if</b> fileexists(fname) <b>then</b>
<b>begin</b>
b:=3DTBitMap.create;
    b.loadfromfile(fname);
<font color=3D"#003399"><i>{b.pixelformat:=3Dpf24bit;}</i></font> <font col=
or=3D"#003399"><i>{to force true black background}</i></font>
moonbtn.visible:=3Dtrue; <font color=3D"#003399"><i>{OK to show the button}=
</i></font>
<font color=3D"#003399"><i>{trim image to circle}</i></font>
<b>for</b> i:=3D0 <b>to</b> b.width-1 <b>do</b>
<b>for</b> j:=3D 0 <b>to</b> b.height-1 <b>do</b>
<b>if</b> trunc(sqrt((cx-i)*(cx-i)+(cy-j)*(cy-j)))&gt;=3Drad
<b>then</b> b.canvas.pixels[i,j]:=3Dclblack;
<b>end</b>;
  doublebuffered:=3Dtrue;
  runflag:=3Dtrue;
  angle:=3D0;
  inc:=3Dpi/64;
<font color=3D"#003399"><i>{loop to set moon phase info}</i></font>
<b>repeat</b>
angle:=3Dangle+inc;
<b>if</b> angle&gt;=3D2*pi <b>then</b> angle:=3Dangle-2*pi;
    moonphase:=3D(1+cos(angle))/2;
<b>if</b> angle&gt;=3DPi <b>then</b> waxing:=3Dtrue
<b>else</b> waxing:=3Dfalse;
    pbox.invalidate; <font color=3D"#003399"><i>{force redraw}</i></font>
application.processmessages;
    sleep(50);
<b>until</b> runflag=3Dfalse;<font color=3D"#003399"><i>
</i></font>  b.free;
<b>end</b>;

<b>procedure</b> swap(<b>var</b> a,b:integer);
<font color=3D"#003399"><i>{exchange 2 integers}</i></font>
<b>var</b> n:integer;
<b>begin</b>  n:=3Da; a:=3Db; b:=3Dn; <b>end</b>;

<font color=3D"#003399"><i>{****************** PBoxPaint *****************}=
</i></font>
<b>procedure</b> TForm1.PBoxPaint(Sender: TObject);
<b>var</b>
delta:integer; <font color=3D"#003399"><i>{x distance from center edge of i=
mage to center edge of arc}</i></font>
fillfrom:integer;  <font color=3D"#003399"><i>{x coordinate for floodfill}<=
/i></font>
ds,de:integer;  <font color=3D"#003399"><i>{y coord start and end points fo=
r arc}</i></font>
myblack:integer;
<b>begin</b>
<b>with</b> PBox,canvas <b>do</b>
<b>begin</b>
<font color=3D"#003399"><i>{make the radius of the crescent vary from "rad"=
 down to 0 as moonphase
     varies from 0 to 1/2 and then back to "rad" as phase goes to 1}</i></f=
ont>
delta:=3Dtrunc(rad*2*abs(moonphase-0.5));
<b>if</b> showmoon <b>then</b>
<b>begin</b>
draw(0,0,b); <font color=3D"#003399"><i>{draw moon image}</i></font>
brush.style:=3DbsClear; <font color=3D"#003399"><i>{let ellipse draw only t=
he border}</i></font>
myblack:=3Db.canvas.pixels[1,1];
<b>end</b>
<b>else</b>
<b>begin</b>
myblack:=3Dclblack;
      brush.color:=3Dmyblack;
      rectangle(0,0,width,height);
      brush.color:=3D$C0E0E0; <font color=3D"#003399"><i>{B-G-R value $C0E0=
E0 =3D light GOLD}</i></font>
<b>end</b>;
    pen.color:=3Dmyblack;
    ellipse(Lx,TY,RX,By);
    ds:=3Dty-1;  <font color=3D"#003399"><i>{set arc start and end points}<=
/i></font>
de:=3Dby+1;
<b>if</b> waxing <b>then</b> <font color=3D"#003399"><i>{increasing moon}</=
i></font>
<b>begin</b>
<font color=3D"#003399"><i>{x coordinate for floodfill to black-out left si=
de for waxing moon}</i></font>
fillfrom:=3Dlx+2;
<font color=3D"#003399"><i>{arc draws counter-clockwise, so to draw right h=
alf of ellipse, start at bottom}</i></font>
<b>if</b> moonphase&lt;0.5 <b>then</b> swap(ds,de);

<b>end</b>
<b>else</b> <font color=3D"#003399"><i>{waning (decreasing) moon}</i></font=
>
<b>begin</b>
fillfrom:=3Drx-2; <font color=3D"#003399"><i>{set right side point for floo=
dfill}</i></font>
<font color=3D"#003399"><i>{same thing - waning moon with over 50% showing,=
 draw right portion of ellipse}</i></font>
<b>if</b> moonphase&gt;0.5 <b>then</b> swap(ds,de)
<b>end</b>;
<b>if</b> delta&gt;1
<b>then</b> arc(cx-delta,ty,cx+delta,BY,cx,ds,cx,de)
<b>else</b> <font color=3D"#003399"><i>{ellipse too narrow to draw, use a l=
ine}</i></font>
<b>begin</b>
moveto(cx,ty);
     lineto(cx,by);
<b>end</b>;
    brush.color:=3Dmyblack;
<b>If</b> moonphase&lt;0.99 <b>then</b> floodfill(fillfrom,cy,myblack,fsbor=
der);
<font color=3D"#003399"><i>{pixels[fillfrom,cy]:=3Dcllime;}</i></font> <fon=
t color=3D"#003399"><i>{for debugging}</i></font>
<b>end</b>;
<b>end</b>;
<font color=3D"#003399"><i>{********************* CloseBtnClick ***********=
**}</i></font>
<b>procedure</b> TForm1.CloseBtnClick(Sender: TObject);
<b>begin</b>
close;
<b>end</b>;

<font color=3D"#003399"><i>{******************* FormClose *****************=
}</i></font>
<b>procedure</b> TForm1.FormClose(Sender: TObject; <b>var</b> Action: TClos=
eAction);
<b>begin</b>
runflag:=3Dfalse;
  action:=3Dcafree;
<b>end</b>;

<font color=3D"#003399"><i>{*********************** MoonBtnClick **********=
***}</i></font>
<b>procedure</b> TForm1.MoonBtnClick(Sender: TObject);
<font color=3D"#003399"><i>{Set/reset flag to show moon image}</i></font>
<b>begin</b>
<b>if</b> <b>not</b> showmoon <b>then</b>
<b>begin</b>
showmoon:=3Dtrue;
    moonbtn.caption:=3D<font color=3D"#9933CC">'Hide moon image'</font>;
<b>end</b>
<b>else</b>
<b>begin</b>
showmoon:=3Dfalse;
    moonbtn.caption:=3D<font color=3D"#9933CC">'Show moon image'</font>;
<b>end</b>;
<b>end</b>;

<b>end</b>.

