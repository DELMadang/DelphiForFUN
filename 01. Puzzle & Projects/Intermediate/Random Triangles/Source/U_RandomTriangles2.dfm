      object Memo2: TMemo
        Left = 8
        Top = 16
        Width = 345
        Height = 385
        Color = 14548991
        Font.Charset = DEFAULT_CHARSET
        Font.Color = clWindowText
        Font.Height = -16
        Font.Name = 'MS Sans Serif'
        Font.Style = [fsBold]
        Lines.Strings = (
          'Divide a stick randomly into three pieces.'
          ''
          'What is the probability that the pieces can '
          'be rearranged to form a triangle?'
          ''
          'Common sense might say that the  '
          'probability is 1/2 since the first cut will '
          'always leave one piece longer than half '
          'the total stick  length.   And half the time, '
          'the second cut will be in  the shorter half, '
          'in which case a forming a triangle will not '
          'be possible.  The other half of the time we '
          'will be cutting the longer piece and a '
          'triangle will be possible.'
          ''
          'But common sense doesn'#39't always'
          'guarantee a correct conclusion! '
          ' '
          ' ')
        ParentFont = False
        TabOrder = 2
      end
