  object Memo3: TMemo
    Left = 24
    Top = 432
    Width = 377
    Height = 201
    Font.Charset = DEFAULT_CHARSET
    Font.Color = clWindowText
    Font.Height = -11
    Font.Name = 'MS Sans Serif'
    Font.Style = [fsBold]
    Lines.Strings = (
      'These cards use binary coding to perform a small "magic '
      'trick".  Think of a number between 0 and 15, then invert the '
      'cards which do not contain your chosen number.  The light green '
      
        'areas on the cards represent holes.  If you stack the cards neat' +
        'ly '
      'and lay them on the blue card, your number will be the only one '
      'showing! '
      ''
      'You can use the "Print" button to  make a set of physical '
      'cards or use these electronic versions.  Just use a sharp '
      'knife to cut out the green areas.: '
      ''
      'CLICK AN ARROW TO ROTATE a card based on whether '
      'your number is on the card or not. When cards are rotated '
      'correctly,  drag each card to the blue card to see the effect.')
    ParentFont = False
    TabOrder = 7
    OnStartDrag = StartDrag
  end
