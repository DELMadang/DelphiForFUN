    object Memo1: TMemo
      Left = 8
      Top = 8
      Width = 209
      Height = 329
      BorderStyle = bsNone
      Color = clYellow
      Lines.Strings = (
        'As everyone knows, the sum of the '
        'cubes of any number of '
        'consecutive integers starting from 1 '
        'is the square of some integer.  Or '
        'did you?  (For example '
        '1^3+2^3=9=3^2,   '
        '1^3+2^3+3^3=16=4^2,  etc.)'
        ''
        'The image at left is a  geometrical '
        'picture of this relationship.  '
        ''
        'How about finding the first sum  of '
        'four or more consecutive cubes '
        '(not starting  with 1) that is the '
        'square of an integer?'
        ''
        '(You can limit starting numbers to '
        '20 or less and the number of  '
        'consecutive integers to 100 or less)')
      TabOrder = 0
    end
  end
